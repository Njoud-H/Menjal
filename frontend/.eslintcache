roduct/:_id\"> \r\n            <NavBar isLoggedIn={auth.isLoggedIn} loginCallback={userLogin}/>\r\n              <EditProduct/>\r\n              <Footer />\r\n            </Route>\r\n            \r\n            <Route path = \"/Add-Product\"> \r\n            <NavBar isLoggedIn={auth.isLoggedIn} loginCallback={userLogin}/>\r\n            <AddProduct \r\n            setAuth={setAuth} //delete\r\n            auth = {auth}/>\r\n            <Footer />\r\n            </Route>\r\n\r\n            \r\n            {/*<Route path = \"/Product\">\r\n              <Product />\r\n            </Route> */}\r\n           \r\n            <Route path = \"/FarmOwner\">\r\n              <NavBar isLoggedIn={auth.isLoggedIn}  loginCallback={userLogin}\r\n              data={dataLoading}/>\r\n                            <FarmOwner \r\n                setAuth={setAuth} \r\n                auth = {auth} \r\n                products={products}/>\r\n                <Footer />\r\n            </Route> \r\n            \r\n            </>\r\n            :\r\n            <>\r\n            <Route path=\"/Customer\"> \r\n              <Customer \r\n              setAuth={setAuth} \r\n              auth = {auth}\r\n              products={products} />\r\n            </Route>\r\n\r\n\r\n\r\n            <Route path=\"/Cart\"> \r\n              <Cart />  \r\n            </Route> \r\n\r\n            <Route path=\"/FavoriteProduct\"> \r\n              <FavoriteProduct \r\n              setAuth={setAuth} \r\n              auth = {auth} />  \r\n            </Route>\r\n\r\n{/* edit profile farmer & customer */}\r\n            <Route path = \"/Profile\">\r\n              <NavBar isLoggedIn={auth.isLoggedIn} loginCallback={userLogin} />\r\n              <Profile />     \r\n              <Footer />\r\n            </Route>\r\n            </>\r\n          }\r\n            </> : <></>\r\n               }\r\n\r\n              <Route exact path = \"/\">\r\n              <Home />\r\n              </Route>\r\n\r\n     